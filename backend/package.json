{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "main": "dist/server.js",
  "scripts": {
    "start": "node server",
    "build:prod": "rimraf dist && cross-env NODE_ENV=production webpack --config ./config/webpack.production.config.js --progress --profile --colors",
    "build:dev": "rimraf dist && cross-env NODE_ENV=dev webpack --config ./config/webpack.config.js --progress --profile --colors",
    "build": "npm run build:dev",
    "start:dev": "npm run build && cd dist && node server.js",
    "start:prod": "npm run build:prod && cd dist && node server",
    "eslint": "eslint ."
  },
  "dependencies": {
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.15.5",
    "jade": "~1.11.0",
    "lodash": "^4.17.4",
    "morgan": "~1.9.0",
    "ramda": "^0.25.0",
    "serve-favicon": "~2.4.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.1.1",
    "eslint": "^4.13.1",
    "eslint-config-nodejs": "^2.0.0",
    "eslint-plugin-jest": "^21.4.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "jest": "^21.2.1",
    "rimraf": "^2.6.2",
    "stats-webpack-plugin": "^0.6.1",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.1",
    "webpack-hot-middleware": "^2.21.0",
    "webpack-node-externals": "^1.6.0"
  }
}
